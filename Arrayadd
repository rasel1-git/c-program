int main(){
int i,n,a[100],sum=0;
printf("\n Enter the range of array: ");
scanf("%d",&n);
printf("\n Enter the Element of array ");
for(i=0;i<n;i++)
scanf("%d",&a[i]);
printf("\n After traversing .....\n");
for(i=0;i<n;i++)
printf("%4d\n",a[i]);


for(i=0; i<n; i++)
    {
        sum+=a[i];
    }
     printf("sum of array is : %d",sum);
}
